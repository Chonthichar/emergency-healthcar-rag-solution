    {
      "cell_type": "code",
      "source": [
        "import subprocess\n",
        "import time\n",
        "import os\n",
        "import atexit\n",
        "from pyngrok import ngrok, conf\n",
        "\n",
        "# --- Configuration ---\n",
        "# Replace with your actual ngrok authentication token\n",
        "NGROK_AUTH_TOKEN = \"30TKwrVFz8v4x145t1MZuN9yYN9_e71qfULdjqBC38s7J8yP\"\n",
        "# The port your FastAPI application will run on\n",
        "FASTAPI_PORT = 9005\n",
        "\n",
        "# --- Process Management ---\n",
        "# A list to keep track of all the processes we start\n",
        "processes = []\n",
        "\n",
        "def cleanup():\n",
        "    \"\"\"\n",
        "    This function is registered to run when the script exits.\n",
        "    It ensures all background processes are properly terminated.\n",
        "    \"\"\"\n",
        "    print(\"\\n--- Shutting down all background processes ---\")\n",
        "    for p in processes:\n",
        "        try:\n",
        "            p.terminate()  # Send a termination signal\n",
        "            p.wait(timeout=5)  # Wait up to 5 seconds for it to close\n",
        "        except subprocess.TimeoutExpired:\n",
        "            p.kill()  # Force kill if it doesn't terminate\n",
        "            print(f\"Force-killed process {p.pid}\")\n",
        "    print(\"--- Cleanup complete ---\")\n",
        "\n",
        "# Register the cleanup function to be called automatically on script exit\n",
        "atexit.register(cleanup)\n",
        "\n",
        "\n",
        "# --- 1. Start Ollama Server ---\n",
        "# Using subprocess.Popen is more robust than os.system because it's non-blocking\n",
        "# and gives us an object to manage the process.\n",
        "print(\"🚀 Starting Ollama server...\")\n",
        "try:\n",
        "    ollama_process = subprocess.Popen([\"ollama\", \"serve\"])\n",
        "    processes.append(ollama_process)\n",
        "    print(f\"✅ Ollama server process started with PID: {ollama_process.pid}\")\n",
        "    # Give Ollama a moment to start up before other services try to use it\n",
        "    time.sleep(5)\n",
        "except FileNotFoundError:\n",
        "    print(\"❌ Error: 'ollama' command not found. Make sure Ollama is installed and in your PATH.\")\n",
        "    exit()\n",
        "\n",
        "\n",
        "# --- 2. Start FastAPI Server ---\n",
        "# We also run uvicorn as a separate process for better stability.\n",
        "print(\"🚀 Starting FastAPI server...\")\n",
        "try:\n",
        "    fastapi_process = subprocess.Popen(\n",
        "        [\"uvicorn\", \"api:app\", \"--host\", \"0.0.0.0\", \"--port\", str(FASTAPI_PORT)]\n",
        "    )\n",
        "    processes.append(fastapi_process)\n",
        "    print(f\"✅ FastAPI server process started with PID: {fastapi_process.pid}\")\n",
        "    # Give the API a moment to start up before creating the tunnel\n",
        "    time.sleep(3)\n",
        "except FileNotFoundError:\n",
        "    print(\"❌ Error: 'uvicorn' command not found. Make sure it's installed in your environment (`pip install uvicorn`).\")\n",
        "    exit()\n",
        "\n",
        "\n",
        "# --- 3. Expose via ngrok ---\n",
        "print(\"🚀 Creating ngrok tunnel...\")\n",
        "conf.get_default().auth_token = NGROK_AUTH_TOKEN\n",
        "public_url = ngrok.connect(FASTAPI_PORT, bind_tls=True).public_url\n",
        "print(f\"✅ ngrok tunnel created: {public_url}\")\n",
        "print(f\"✅ API is live at {public_url}\")\n",
        "\n",
        "\n",
        "# --- 4. Keep the main script alive ---\n",
        "print(\"--- Server is running. Press Ctrl+C to quit. ---\")\n",
        "try:\n",
        "    # This loop keeps the main script alive. It will exit if you press Ctrl+C.\n",
        "    while True:\n",
        "        time.sleep(1)\n",
        "except KeyboardInterrupt:\n",
        "    # When you press Ctrl+C, this block is executed.\n",
        "    print(\"\\n👋 KeyboardInterrupt received.\")\n",
        "finally:\n",
        "    # Disconnect the ngrok tunnel\n",
        "    ngrok.disconnect(public_url)\n",
        "    print(\"❌ ngrok tunnel stopped.\")\n",
        "    # The cleanup function registered with atexit will handle the rest.\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 1000
